#include <stdio.h>

int linearSearch(int arr[], int size, int target) {
    for (int i = 0; i < size; i++) {
        if (arr[i] == target)
            return i;
    }
    return -1;
}

int main() {
    int arr[] = {10, 25, 30, 45, 60};
    int size = sizeof(arr) / sizeof(arr[0]);

    int index = linearSearch(arr, size, 30);
    printf("%d\n", index);

    index = linearSearch(arr, size, 99);
    printf("%d\n", index);

    return 0;
}
